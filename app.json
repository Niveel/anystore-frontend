{
  "expo": {
    "name": "Shopwit",
    "slug": "Shopwit",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/a_icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/custom_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a2f56"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Camera access is required to scan Bar/QR Codes to perform searches. Results are for your viewing only and are not saved or shared by Shopwit."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.nivee.shopwit",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/a_icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nivee.shopwit"
    },
    "web": {
      "favicon": "./app/assets/a_icon.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "f16d98e9-37c8-4f09-80cc-48b39654da97"
      }
    },
    "owner": "storebuffet",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
